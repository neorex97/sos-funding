AACA,QAAA,OAAA,CAAA,GAAA,CAAA,EAAA;AACA,OAPA,CAQA;;AACA,KA9BA;AA+BA,IAAA,KA/BA,mBA+BA;AAAA;;AACA,WAAA,MAAA,GAAA,KAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,KAAA,CAAA,WAAA,CAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA,EAGA,GAHA,CAAA;AAIA,KArCA;AAsCA,IAAA,IAtCA,kBAsCA;AACA,UAAA,EAAA,GAAA,KAAA,OAAA,CAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,EAAA;;AACA,UAAA,KAAA,WAAA,GAAA,CAAA,CAAA,EAAA;AACA,QAAA,WAAA,CAAA,UAAA,CAAA,EAAA,EAAA,KAAA,OAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,OAAA,EAFA,CAGA;AACA,OAJA,MAIA;AACA,YAAA,QAAA,GAAA,WAAA,CAAA,kBAAA,CAAA,KAAA,OAAA,CAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,EAFA,CAGA;AACA;;AACA,WAAA,KAAA;AACA;AAnDA;AAvFA,CAAA","sourcesContent":["<template>\n<v-card>\n  <!-- OUTER TABLE -->\n  <v-data-table\n    :headers=\"headers\"\n    :items=\"sosfunding\"\n  >\n  <!-- Custom headers -->\n   <template v-slot:header.sos=\"{ header }\">\n      {{ header.text.toUpperCase()}}\n    </template>\n    <template v-slot:header.others=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    <template v-slot:header.subtotal=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    <template v-slot:header.total=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    <template v-slot:header.actions=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n\n  <template v-slot:item=\"props\">\n    <tr>\n          <!-- Tooltip message on sos -->\n\n       <v-tooltip bottom>\n      <template v-slot:activator=\"{ on }\">\n      <td v-on=\"on\">\n          <!-- Textfied on dialog for editing Sos contribution -->\n\n       <v-edit-dialog\n          :return-value.sync=\"props.item.sosamount\"\n        > {{ props.item.sosamount }} {{sosPercentage}}\n          <template v-slot:input >\n            <v-form @submit=\"editsos(props.item)\">\n            <v-text-field\n              v-model=\"props.item.sosamount\"\n              :rules=\"[max25chars]\"\n              label=\"sos contribution\"\n              single-line\n              counter\n            ></v-text-field>\n            </v-form>\n          </template>\n        </v-edit-dialog>\n        </td>\n        </template>\n      <span>Sos contribution</span>\n    </v-tooltip>\n\n  <!-- INNER TABLE  -->\n\n     <v-data-table \n     :headers=\"innerheaders\"\n     :items=\"others\"\n     \n      hide-default-footer>\n      <!-- Custom headers -->\n      <template v-slot:header.name=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    <template v-slot:header.description=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    <template v-slot:header.amount=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    <template v-slot:header.actions=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    \n      <template v-slot:item=\"props\">\n      <tr>\n        <!-- Data from other contributions -->\n\n        <td>{{ props.item.name }}</td>\n        <td>{{ props.item.description }}</td>\n        <td>{{ props.item.amount }}</td>\n\n    <!-- Edit delete actions     -->\n    <td> \n       <v-icon\n        small\n        class=\"mr-2\"\n        @click=\"editItem(props.item)\"\n      >\n        mdi-pencil\n      </v-icon>\n      <v-icon\n        small\n        @click=\"deleteItem(props.item.id)\"\n      >\n        mdi-delete\n      </v-icon>\n      </td>\n          </tr>  \n \n \n      </template>\n\n    <!-- Pop-up form to edit and add a item -->\n\n      <template v-slot:top>\n      <v-toolbar flat color=\"white\">\n        <v-spacer></v-spacer>\n        <v-dialog v-model=\"dialog\" max-width=\"800px\">\n          <template v-slot:activator=\"{ on }\">\n            <v-btn color=\"primary\" dark class=\"mb-2\" v-on=\"on\">Add new contribution</v-btn>\n          </template>\n          <v-card>\n            <v-card-title>\n              <span class=\"headline\">{{ formTitle }}</span>\n            </v-card-title>\n\n            <v-card-text>\n              <v-container>\n                <v-row>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"newItem.name\" label=\"Name of the organization\" ></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"newItem.description\" label=\"Description\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"newItem.amount\" label=\"Amount\"></v-text-field>\n                  </v-col>\n                 \n                </v-row>\n              </v-container>\n            </v-card-text>\n\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn color=\"blue darken-1\" text @click=\"close\">Cancel</v-btn>\n              <v-btn color=\"blue darken-1\" text @click=\"save\">Save</v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-dialog>\n      </v-toolbar>\n    </template>\n    \n    </v-data-table>\n      <td class=\"text-xs-right\">{{ countSubtotal }} {{subPercent}} </td> \n      <td class=\"text-xs-right\">{{ countTotal }}</td>\n      \n      </tr>\n  </template>\n  \n\n  </v-data-table>\n  </v-card>\n</template>\n\n<script>\nimport ApiServices from '@/services/ApiServices'\nimport {mapState,mapGetters,mapMutations,mapActions} from 'vuex'\n  export default {\n    data: () => ({\n      dialog: false, //to control pop-up form\n      headers: [\n        {\n          text: 'SOS contribution sought in this application',\n          align: 'start',\n          sortable: false,\n          value: 'sos',\n        },\n        { text: 'Other contributions', value: 'others' },\n        { text: 'Subtotal of other contributions', value: 'subtotal' },\n        { text: 'Total contributions', value:'total' },\n        \n      ],\n       innerheaders: [\n        {\n          text: 'Name',\n          align: 'start',\n          sortable: false,\n          value: 'name',\n        },\n        { text: 'Description', value: 'description' },\n        { text: 'amount (EUR)', value: 'amount' },\n        { text: 'Actions', value: 'actions', sortable: false },\n      ],\n      \n      editedIndex: -1,\n      defaultsos:{\n          sosid:'',\n        sosamount:''\n      },\n      newsos:{\n        sosid:'',\n        sosamount:''\n      },\n      newItem: {\n        id: '',\n        name: '',\n        description:'',\n        amount:'',\n      },\n      defaultItem: {\n        id : '',\n        name: '',\n        description:'',\n        amount:'',\n        \n      },\n    }),\n  \n    computed: {\n     ...mapState([\n        'sosfunding',\n        'others',\n    ]),\n     ...mapGetters([\n  'countSubtotal',\n  'countTotal',\n  'sosPercentage',\n  'subPercent'\n  \n  ]),\n      formTitle () {\n        return this.editedIndex === -1 ? 'New Item' : 'Edit Item'\n      },\n      \n    },\n    watch: {\n      dialog (val) {\n        val || this.close()\n      },\n    },\n\n    // created () {\n    //   this.initialize()\n    // },\n\n    //call actions when component is mounted to get data from server\n   mounted() {\n      this.$store.dispatch('GetSos')\n      this.$store.dispatch('GetOtherFunds')\n    },\n    //update after every change\n    // updated() {\n    //   this.$store.dispatch('GetOtherFunds')\n    // },\n    methods: {\n     ...mapActions([\n      'removeItem',\n      'addNewItem',\n      'GetSos',\n      'GetOtherFunds',\n    ]),\n    editsos(item){\n      this.newsos = Object.assign({}, item)\n      var id = this.newsos.sosid\n      console.log(id)\n      ApiServices.updateSos(id,this.newsos)\n      this.newsos = Object.assign({}, this.defaultsos)   \n    },\n    //store new data for adding new row  or editing one\n     editItem (item) {\n        this.editedIndex = this.others.indexOf(item)\n        this.newItem = Object.assign({}, item)\n        this.dialog = true\n      },\n      //call delete api\n        deleteItem (Id) {\n        var id = Id\n        //const id = this.item.id\n        var r = confirm('Are you sure you want to delete this item?')\n        if(r==true){\n           ApiServices.deleteItem(id)\n           console.log(id)\n        }\n        //this.removeItem(index)\n      },\n        close () {\n        this.dialog = false\n        setTimeout(() => {\n          this.newItem = Object.assign({}, this.defaultItem)\n          this.editedIndex = -1\n        }, 300)\n      },\n        save () {\n          var id = this.newItem.id\n          console.log(id)\n        if (this.editedIndex > -1) {\n          ApiServices.updateItem(id,this.newItem)\n            console.log(this.newitem)\n          //Object.assign(this.others[this.editedIndex], this.newItem)\n        } else {\n          const response =ApiServices.addNewcontribution(this.newItem)\n            console.log(response.data)\n          //this.addNewItem(this.newItem) \n        }\n        this.close()\n      },\n    },\n  }\n</script>\n\n// Data is stored inside store/index.js vuex is used for state management and accessing data   "],"sourceRoot":"src/components"}]}