AA,WAAA,CAAA,SAAA,CAAA,EAAA,EAAA,KAAA,MAAA;AACA,WAAA,MAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,KAAA,UAAA,CAAA;AACA,KAjCA;AAkCA;AACA,IAAA,QAnCA,oBAmCA,IAnCA,EAmCA;AACA,WAAA,WAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,WAAA,OAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,IAAA,CAAA;AACA,WAAA,MAAA,GAAA,IAAA;AACA,KAvCA;AAwCA;AACA,IAAA,UAzCA,sBAyCA,EAzCA,EAyCA,IAzCA,EAyCA;AACA,UAAA,KAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,UAAA,CAAA,GAAA,OAAA,CAAA,4CAAA,CAAA;;AACA,UAAA,CAAA,IAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,EAAA;AACA,QAAA,WAAA,CAAA,UAAA,CAAA,EAAA;AACA,aAAA,gBAAA,CAAA,KAAA;AACA;AACA,KAjDA;AAkDA,IAAA,KAlDA,mBAkDA;AAAA;;AACA,WAAA,MAAA,GAAA,KAAA;AACA,MAAA,UAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,MAAA,CAAA,WAAA,CAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA,EAGA,GAHA,CAAA;AAIA,KAxDA;AAyDA,IAAA,IAzDA,kBAyDA;AAAA;;AACA,UAAA,EAAA,GAAA,KAAA,OAAA,CAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,EAAA;;AACA,UAAA,KAAA,WAAA,GAAA,CAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,KAAA,MAAA,CAAA,KAAA,WAAA,CAAA,EAAA,KAAA,OAAA;AACA,QAAA,WAAA,CAAA,UAAA,CAAA,EAAA,EAAA,KAAA,OAAA;AACA,OAHA,MAIA;AACA,QAAA,WAAA,CAAA,kBAAA,CAAA,KAAA,OAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;;AACA,UAAA,MAAA,CAAA,aAAA,CAAA,QAAA,CAAA,IAAA;AACA,SAJA,EAIA,KAJA,CAIA,UAAA,CAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,SANA;AAOA;;AACA,WAAA,KAAA;AACA;AA1EA;AA9EA,CAAA","sourcesContent":["<template>\n<v-card>\n  <!-- OUTER TABLE -->\n  <v-data-table\n    :headers=\"headers\"\n    :items=\"sosfunding\"\n  >\n  <!-- Custom headers -->\n   <template v-slot:header.sos=\"{ header }\">\n      {{ header.text.toUpperCase()}}\n    </template>\n    <template v-slot:header.others=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    <template v-slot:header.subtotal=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    <template v-slot:header.total=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    <template v-slot:header.actions=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n\n  <template v-slot:item=\"props\">\n    <tr>\n          <!-- Tooltip message on sos -->\n\n       \n      <td>\n<!-- Textfied on dialog for editing sosamount -->\n       <v-edit-dialog\n          :return-value.sync=\"props.item.sosamount\"\n        > {{ props.item.sosamount }} {{sosPercentage}}\n          <template v-slot:input >\n            <v-form @submit=\"editsos(props.item)\">\n            <v-text-field\n              \n              v-model=\"props.item.sosamount\"\n              :rules=\"[max25chars]\"\n              label=\"sos contribution\"\n              single-line\n              counter\n            ></v-text-field>\n            </v-form>\n          </template>\n        </v-edit-dialog>\n        </td>\n     \n\n  <!-- INNER TABLE  -->\n\n     <v-data-table \n     :headers=\"innerheaders\"\n     :items=\"others\"\n     \n      hide-default-footer>\n      <!-- Custom headers -->\n      <template v-slot:header.name=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    <template v-slot:header.description=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    <template v-slot:header.amount=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    <template v-slot:header.actions=\"{ header }\">\n      {{ header.text.toUpperCase() }}\n    </template>\n    \n      <template v-slot:item=\"props\">\n      <tr>\n        <!-- Data from other contributions -->\n\n        <td>{{ props.item.name }}</td>\n        <td>{{ props.item.description }}</td>\n        <td>{{ props.item.amount }}</td>\n\n    <!-- Edit delete actions     -->\n    <td> \n       <v-icon\n        small\n        class=\"mr-2\"\n        @click=\"editItem(props.item)\"\n      >\n        mdi-pencil\n      </v-icon>\n      <v-icon\n        small\n        @click=\"deleteItem(props.item.id,props.item)\"\n      >\n        mdi-delete\n      </v-icon>\n      </td>\n          </tr>  \n \n \n      </template>\n\n    <!-- Pop-up form to edit and add a item -->\n\n      <template v-slot:top>\n      <v-toolbar flat color=\"white\">\n        <v-spacer></v-spacer>\n        <v-dialog v-model=\"dialog\" max-width=\"800px\">\n          <template v-slot:activator=\"{ on }\">\n            <v-btn color=\"primary\" dark class=\"mb-2\" v-on=\"on\">Add new contribution</v-btn>\n          </template>\n          <v-card>\n            <v-card-title>\n              <span class=\"headline\">{{ formTitle }}</span>\n            </v-card-title>\n  \n            <v-card-text>\n              <v-container>\n                <v-row>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"newItem.name\" label=\"Name of the organization\" ></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"newItem.description\" label=\"Description\"></v-text-field>\n                  </v-col>\n                  <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                    <v-text-field v-model=\"newItem.amount\" label=\"Amount\"></v-text-field>\n                  </v-col>\n                 \n                </v-row>\n              </v-container>\n            </v-card-text>\n\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn color=\"blue darken-1\" text @click=\"close\">Cancel</v-btn>\n              <v-btn color=\"blue darken-1\" text @click=\"save\">Save</v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-dialog>\n      </v-toolbar>\n    </template>\n    \n    </v-data-table>\n      <td class=\"text-xs-right\">{{ countSubtotal }} {{subPercent}} </td> \n      <td class=\"text-xs-right\">{{ countTotal }}</td>\n      \n      </tr>\n  </template>\n  \n\n  </v-data-table>\n  \n  </v-card>\n</template>\n\n<script>\nimport ApiServices from '@/services/ApiServices'\nimport {mapState,mapGetters,mapMutations,mapActions} from 'vuex'\n  export default {\n    data: () => ({\n      dialog: false, //to control pop-up form\n      headers: [\n        {\n          text: 'SOS contribution sought in this application',\n          align: 'start',\n          sortable: false,\n          value: 'sos',\n        },\n        { text: 'Other contributions', value: 'others' },\n        { text: 'Subtotal of other contributions', value: 'subtotal' },\n        { text: 'Total contributions', value:'total' },\n        \n      ],\n       innerheaders: [\n        {\n          text: 'Name',\n          align: 'start',\n          sortable: false,\n          value: 'name',\n        },\n        { text: 'Description', value: 'description' },\n        { text: 'amount (EUR)', value: 'amount' },\n        { text: 'Actions', value: 'actions', sortable: false },\n      ],\n      \n      editedIndex: -1,\n      defaultsos:{\n          sosid:'',\n        sosamount:''\n      },\n      newsos:{\n        sosid:'',\n        sosamount:''\n      },\n      newItem: {\n        id: '',\n        name: '',\n        description:'',\n        amount:'',\n      },\n      defaultItem: {\n        id : '',\n        name: '',\n        description:'',\n        amount:'',\n        \n      },\n    }),\n  \n    computed: {\n     ...mapState([\n        'sosfunding',\n        'others',\n    ]),\n     ...mapGetters([\n  'countSubtotal',\n  'countTotal',\n  'sosPercentage',\n  'subPercent'\n  \n  ]),\n      formTitle () {\n        return this.editedIndex === -1 ? 'Add new contribution' : 'Edit Item'\n      },\n      \n    },\n    watch: {\n      dialog (val) {\n        val || this.close()\n      },\n    },\n//invoke methods to get data from server when component is mounted\n    mounted: function () {\n    this.Sos()\n    this.Others()\n    },\n  methods: {\n     ...mapActions([\n      'removeItemAction',\n      'addItemAction',\n      'getSosAction',\n      'getOtherAction'\n    ]),\n  //GET DATA FROM SERVER\n    Sos(){\n      ApiServices.getsosfunds()\n      .then(response => {\n        console.log(response.data)\n        this.getSosAction(response.data)\n      }).catch(e => {\n        console.log(e);\n        });\n    },\n    Others(){\n     ApiServices.getotherfunds()\n      .then(response => {\n        console.log(response.data)\n        this.getOtherAction(response.data)  \n      }).catch(e => {\n          console.log(e);\n        });\n    },\n    //Update sosamount\n    editsos(item){\n        this.newsos = Object.assign({}, item)\n        var id = this.newsos.sosid\n        console.log(id)\n        ApiServices.updateSos(id,this.newsos)\n        this.newsos = Object.assign({}, this.defaultsos)   \n    },\n    //store edit data to newItem and set editIndex to determine which action to perform edit/create\n     editItem (item) {\n        this.editedIndex = this.others.indexOf(item)\n        this.newItem = Object.assign({}, item)\n        this.dialog = true\n      },\n    // Delete selected row using its id \n    deleteItem (id,item) {\n        var index = this.others.indexOf(item)\n        var r = confirm('Are you sure you want to delete this item?')\n        if(r==true){\n          console.log(id)\n           ApiServices.deleteItem(id)\n           this.removeItemAction(index)\n        }  \n      },\n    close () {\n        this.dialog = false\n        setTimeout(() => {\n          this.newItem = Object.assign({}, this.defaultItem)\n          this.editedIndex = -1\n        }, 300)\n      },\n    save () {\n          var id = this.newItem.id\n          console.log(id)\n        if (this.editedIndex > -1) {\n          Object.assign(this.others[this.editedIndex], this.newItem)\n          ApiServices.updateItem(id,this.newItem)\n        } \n        else {\n          ApiServices.addNewcontribution(this.newItem)\n          .then(response => {\n          console.log(response.data)\n          this.addItemAction(response.data) \n        }).catch(e => {\n          console.log(e);\n        });\n        }\n        this.close()\n      },\n    },\n  }\n</script>\n\n// Data is stored inside store/index.js vuex is used for state management and accessing data   "],"sourceRoot":"src/components"}]}